{
	"projects": [
		{
			"displayName": "unit",
			"preset": "ts-jest",
			"testEnvironment": "node",
			"testMatch": ["<rootDir>/test/unit/**"]
		},
		{
			"displayName": "integ",
			"preset": "ts-jest",
			"testEnvironment": "node",
			"testMatch": ["<rootDir>/test/integ/**"]
		}
	],
	"collectCoverage": true,
	"collectCoverageFrom": ["src/**/*.ts", "!**/index.ts"],
	"errorOnDeprecated": true
}
